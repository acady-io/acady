{
  "name": "acady",
  "version": "0.0.1-dev",
  "description": "",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc && chmod +x ./build/bin/acady.js",
    "prepare": "npm run build",
    "test": "npm run prepare && jest",
    "dev": "npm run build && node ./build/bin/acady.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/sha256": "^0.2.0",
    "jest": "^26.6.3",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acady-io/acady.git"
  },
  "author": "Christian Schab <christian.schab@web.academy>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.809.0",
    "cli-table": "^0.3.4",
    "commander": "^6.2.0",
    "gluegun": "^4.5.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "log-symbols": "^4.0.0",
    "nanoid": "^3.1.20",
    "node-fetch": "^2.6.1"
  },
  "bin": {
    "acady": "./build/bin/acady.js"
  },
  "keywords": [
    "serverless",
    "web.academy",
    "acady",
    "cli",
    "devtools",
    "webdev",
    "devops"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "canary",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
